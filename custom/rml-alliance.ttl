@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix dcterms:<http://purl.org/dc/terms/> .
@prefix locn:   <http://www.w3.org/ns/locn#>.
@prefix kds: <http://knowledgelinks.io/ns/data-structures/> .
@prefix oslo:   <http://purl.org/oslo/ns/localgov#>.
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix relators: <http://id.loc.gov/vocabulary/relators/> .
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix schema: <http://schema.org/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.


<#MARC2BIBFRAME_AllianceMatchKey> a rr:TriplesMap ;
    
    rml:logicalSource [
        rml:source "{marc_record}" ;
        rml:referenceFormulation ql:XPath ;
        rml:iterator "marc:datafield[@tag='997']"

    ] ;

    rr:subjectMap [
        rr:termType rr:BlankNode ;
        rr:class bf:Identifier ;
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdfs:label ;
        rr:objectMap [
            rr:constant "Colorado Alliance Match Key"@en ;
            rr:datetype xsd:string
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate rdf:value ;
        rr:objectMap [
            rr:reference "./marc:subfield[@code='a']"
        ]
    ] .

<#MARC2BIBFRAME_Item> a rr:TriplesMap ;

    rml:logicalSource [
        rml:source "{marc_record}" ;
        rml:referenceFormulation ql:XPath ;
        rml:iterator "."
    ] ;

    rr:subjectMap [
        rr:template "{item_iri}" ;
        rr:class bf:Item 
    ];

    rr:predicateObjectMap [
        rr:predicate bf:heldBy ;
        rr:objectMap [
            rr:template "{institution_iri}";
            rr:datatype xsd:anyURI
        ]
    ];

    rr:predicateObjectMap [
        rr:predicate bf:itemOf ;
        rr:objectMap [
            rr:template "{instance_iri}" ;
            rr:datatype xsd:anyURI
        ]
    ] ;

    rr:predicateObjectMap [
        rr:predicate schema:sameAs ;
        rr:objectMap [
            rr:template "{ils_url}" ;
            rr:datatype xsd:anyURI
        ]
    ] .


<#MARC2BIBFRAME_Person> a rr:TriplesMap ;

    rml:logicalSource [
        rml:source "{marc_record}" ;
        rml:iterator "."
    ] ;

    rr:subjectMap [
        rr:template "{person_id}" ;
        rr:class bf:Person ;
        kds:deduplicate rdfs:label;
    ] .


    
